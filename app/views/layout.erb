  <style>
    body .topnav {
      background-color: #333;
      overflow: hidden;
    }
   
    .topnav a {
      float: left;
      color: #f2f2f2;
      text-align: center;
      padding: 14px 16px;
      text-decoration: none;
      font-size: 17px;
    }
   
    .topnav a:hover {
      background-color: #ddd;
      color: black;
    }

    
    .topnav a.active {
      background-color: #4CAF50;
      color: white;
    }
  </style>
  <body>
    
    <div class="topnav">
      <a class="active" href="/">Main</a>
      <a href='/users/:id'>My Page</a>
      <a href="/users">Other Collections</a>
      <a href="/logout">Logout</a>
    </div>

          
      

    <div class="wrapper">

        <%= yield %>

      <footer class="branding">
        <small>&copy; 2020 <strong>PinBIn</strong></small>
      </footer>
    </div>

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
